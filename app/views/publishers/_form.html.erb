<%= simple_form_for(@publisher) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :publisherid %>
    <%= f.input :name %>
    <%= f.input :logo %>
    <%= f.input :url %>
    <%= f.input :blurb %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
